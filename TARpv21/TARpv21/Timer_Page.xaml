<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TARpv21.Timer_Page">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lbl" Text="Siin ilmub timer" FontSize="50"  BackgroundColor="DarkCyan">
                <Label.GestureRecognizers>
                    <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped"/>
                </Label.GestureRecognizers>
            </Label>
            <Button x:Name="onoffbtn" Text="Lülita sisse" FontSize="Title" Background="Black" Clicked="onoffbtn_Clicked">
            </Button>
            <Button x:Name="Tagasi" Text="Tagasi" FontSize="Title" Background="Black" Clicked="Tagasi_Clicked">
            </Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>